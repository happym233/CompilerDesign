/* sort the array */ 
function bubbleSort ( arr : integer [ ] , size : integer ) => void 
{ 
localvar n : integer ; 
localvar i : integer ; 
localvar j : integer ; 
localvar temp : integer ; 
n = size ; 
i = 0 ; 
j = 0 ; 
temp = 0 ; 
while ( i < n - 1 ) { 
while ( j < n - i - 1 ) { 
if ( arr [ j ] > arr [ j + 1 ] ) 
then { 
// swap temp and arr[i] 
temp = arr [ j ] ; 
arr [ j ] = arr [ j + 1 ] ; 
arr [ j + 1 ] = temp ; 
} else ; 
j = j + 1 ; 
} ; 
i = i + 1 ; 
} ; 
} 
