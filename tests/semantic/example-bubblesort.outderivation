then { 
// swap temp and arr[i] 
temp = arr [ j ] ; 
arr [ j ] = arr [ j + 1 ] ; 
arr [ j + 1 ] = temp ; 
} else ; 
j = j + 1 ; 
} ; 
i = i + 1 ; 
} ; 
} 
