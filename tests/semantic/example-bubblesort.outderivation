if ( arr [ j ] > arr [ j + 1 ] ) 
then { 
// swap temp and arr[i] 
temp = arr [ j ] ; 
arr [ j ] = arr [ j + 1 ] ; 
arr [ j + 1 ] = temp ; 
} else ; 
j = j + 1 ; 
} ; 
i = i + 1 ; 
} ; 
} 
/* print the array */ 
function printArray ( arr : integer [ ] , size : integer ) => void 
{ 
localvar n : integer ; 
localvar i : integer ; 
n = size ; 
i = 0 ; 
while ( i < n ) { 
write ( arr [ i ] ) ; 
i = i + 1 ; 
} ; 
} 
